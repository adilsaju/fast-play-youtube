{
  "manifest_version": 2,
  "name": "Speed increaser",
 "description": "Speed increaser for youtube",
 "version": "1.0",
 "content_scripts": [
  {
   
  "matches": ["https://www.youtube.com/*"],
   "js": ["content.js"],
   "run_at": "document_end"
  }
 ],


  "browser_action": {
    "default_icon": "12193-200.png",
    "default_title": "Speed increaser",
    "default_popup": "popup.html"
  },

"permissions": [
  "tabs",
  "http://*/*",
  "https://*/*"
]

 }
